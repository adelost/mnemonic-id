{
  "name": "mnemonic-id",
  "description": "Library to generate easy to remember, and sometimes entertaining, human readable ids",
  "version": "4.1.0",
  "license": "MIT",
  "author": "Mattias E. O. Andersson",
  "repository": "github:adelost/mnemonic-id",
  "homepage": "https://github.com/adelost/mnemonic-id#readme",
  "keywords": [
    "id",
    "uuid",
    "mnemonic",
    "random",
    "name",
    "generator",
    "human",
    "readable",
    "sentence",
    "memorable"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "lint": "eslint .",
    "prepublishOnly": "npm run lint && npm run build"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "eslint": "^9.18.0",
    "tsup": "^8.3.0",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.20.0",
    "vitest": "^4.0.18"
  }
}
